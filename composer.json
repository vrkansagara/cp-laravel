{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-snappy": "^0.4.6",
        "commercepundittech/laravel-seeding": "^1.0",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.0",
        "laravel/framework": "^6.2",
        "laravel/socialite": "^4.3",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^2.0",
        "prettus/l5-repository": "^2.6",
        "proengsoft/laravel-jsvalidation": ">2.2.0",
        "spatie/laravel-permission": "^3.7",
        "vrkansagara/lara-out-press": "^2.0",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-oracle": "^9.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laminas/laminas-coding-standard": "^1.0",
        "laravel/ui": "^1.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "./vendor/bin/phpcs",
        "cs-fix": "./vendor/bin/phpcbf",
        "clear": "php artisan route:clear && php artisan view:clear && php artisan config:clear && php artisan permission:cache-reset && rm -rf composer.lock yarn.lock ",
        "post-update-cmd": [
            "php artisan vendor:publish --provider=\"Proengsoft\\JsValidation\\JsValidationServiceProvider\" --tag=public --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
